<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.unab.parra.cristian.reprofunab" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_reunab"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_reunab"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.NavigationDrawerActivity"
            android:label="@string/title_activity_navigation_drawer"
             >

        </activity>
        <activity android:name=".Activities.DetalleActivity" />
        <activity android:name=".Activities.ExamenActivity" />
        <activity android:name=".Activities.AyudaActivity" />
        <activity android:name=".Activities.ConfiguracionActivity"/>
        <activity android:name=".Activities.SplashActivity"
            android:configChanges="uiMode">


        <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>





</manifest>